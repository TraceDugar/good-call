{
  "expo": {
    "name": "tacti-light",
    "slug": "tacti-light",
    "description": "Screen flashlight that is easy to use with haptic feedback, also useable with phone flashlight to turn your phone into a bi-directional light.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/icons8-flashlight-64.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/junior-ferreira-7esRPTt38nI-unsplash.jpg", 
      "resizeMode": "contain",
      "backgroundColor": "#b1b8bd"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/junior-ferreira-7esRPTt38nI-unsplash.jpg",
        "backgroundColor": "#b1b8bd"
      }
    },
    "web": {
      "favicon": "assets/icons8-flashlight-64.png"
    }
  }
}
